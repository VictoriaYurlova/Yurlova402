.str-bg-peach {
    background-color: #ffbb78;
    width: 100%;
    height: 100%;
}
.form-bg-white {
    background-color: white;
    border-radius: 25px;
    position: fixed;
    top: 15%;
    left: 35%;
    padding: 30px;
    width: 25%;
    height: 65%;
}
.form-title {
    text-align: left;
    font-family: cursive;
    font-size: x-large;
}
.input-username {
    background-color: rgb(217, 231, 243);
    border: 1px solid rgb(203, 223, 240);
    border-radius: 10px;
    width: 100%;
    height: 35px;
}
.input-email {
    background-color: rgb(217, 231, 243);
    border: 1px solid rgb(203, 223, 240);
    border-radius: 10px;
    width: 100%;
    height: 35px;
}
.input-ms {
    background-color: rgb(217, 231, 243);
    border: 1px solid rgb(203, 223, 240);
    border-radius: 10px;
    width: 100%;
    height: 150px;
    justify-content:end;
}
.btn {
    padding: 15px;
    border: 1px solid rgb(238, 115, 71);
    border-radius: 10px;
    cursor: pointer;
    text-transform:uppercase;
    color: white;
    background-color: #f3bb86;
    width: 100%;
}
<html>
    <head>
      <link rel="stylesheet" href="style.css">
      <title>Форма</title>
    </head>
    <body>
       <div class="str-bg-peach">
         <form >
             <div class="form-bg-white">
               <div class="form-title"> 
                   Написать нам <br>
               </div>
               <div>
                 <br><input type="text" class="input-username" placeholder="Username"><br>
               </div>
               <div>
                 <br><input type="text" class="input-email" placeholder="Email"> <br>
               </div>
               <div>
                 <br><input type="text" class="input-ms" placeholder="Сообщение...">
               </div>
               <div>
                 <br><button type="button" class="btn">
                    Отправить
                 </button>
               </div>
            </div>
        </form>  
      </div> 
    </body>
</html>
